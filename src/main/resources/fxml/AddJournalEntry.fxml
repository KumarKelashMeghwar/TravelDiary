<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="512.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.traveldiary.controllers.AddJournalEntryController">
   <HBox alignment="CENTER" prefHeight="71.0" prefWidth="645.0">
      <Button focusTraversable="false" mnemonicParsing="false" onMouseClicked="#backBtnClicked" prefHeight="31.0" prefWidth="101.0" text="Go Back">
         <font>
            <Font name="Poppins Regular" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label alignment="CENTER" prefHeight="47.0" prefWidth="315.0" text="Traval Diary">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </HBox>
   <Label layoutX="118.0" layoutY="71.0" prefHeight="31.0" prefWidth="127.0" text="Add Journal Entry" underline="true">
      <font>
         <Font name="Poppins Regular" size="12.0" />
      </font>
   </Label>
   <TextField fx:id="title" focusTraversable="false" layoutX="118.0" layoutY="117.0" prefHeight="31.0" prefWidth="197.0" promptText="Title" />
   <TextField fx:id="locations" focusTraversable="false" layoutX="118.0" layoutY="174.0" prefHeight="31.0" prefWidth="197.0" promptText="Location" />
   <DatePicker fx:id="date" focusTraversable="false" layoutX="341.0" layoutY="114.0" prefHeight="31.0" prefWidth="197.0" promptText="Date" />
   <TextField fx:id="expsenses" focusTraversable="false" layoutX="341.0" layoutY="174.0" prefHeight="31.0" prefWidth="197.0" promptText="Expenses" />
   <TextArea fx:id="description" focusTraversable="false" layoutX="118.0" layoutY="229.0" prefHeight="113.0" prefWidth="421.0" promptText="Description" />
   <TextField fx:id="ratings" focusTraversable="false" layoutX="118.0" layoutY="366.0" prefHeight="31.0" prefWidth="197.0" promptText="Ratings" />
   <Button focusTraversable="false" layoutX="358.0" layoutY="366.0" mnemonicParsing="false" onMouseClicked="#uploadImagesBtnClicked" prefHeight="31.0" prefWidth="181.0" text="Upload Images">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Button focusTraversable="false" layoutX="118.0" layoutY="436.0" mnemonicParsing="false" onMouseClicked="#addEntryBtnClicked" prefHeight="31.0" prefWidth="421.0" text="Add Entry">
      <font>
         <Font name="Poppins Regular" size="14.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
</AnchorPane>
